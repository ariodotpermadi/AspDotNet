@page
@model MyApp.Pages.Scada_BHP.ScadaBHPModel
@{
}


<div class="container body-content">
<div class="card">
    <div class="card-header">
        <h3>Pep Code</h3>
        <div class="card-body">
            <table class="table table-hover">
                <thead>
                    <th>ID</th>
                    <th>Type</th>
                    <th>Parent Code</th>
                    <th>Code Desc</th>
                    <th>Child Code</th>
                    <th>Subcode Desc</th>
                    <th>Pep Code</th>
                </thead>
                <tbody>
                    @foreach (var item in Model.scadaInfos)
                    {
                        <tr>
                            <td>@item.id</td>
                            <td>@item.type</td>
                            <td>@item.parent_code</td>
                            <td>@item.code_desc</td>
                            <td>@item.child_code</td>
                            <td>@item.subcode_desk</td>
                            <td>@item.pep_code</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
</div>